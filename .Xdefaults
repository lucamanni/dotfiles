!{{{ Colors
#include "/home/luca/.colors/Terminal/colored"
URxvt*.depth: 32
URxvt*.background: [92]#080808
!URxvt*.background: #080808
URxvt*.foreground: #ecedee
!}}}
!{{{ Font Settings
Xft.dpi:        92
Xft.antialias:  True
Xft.rgba:       rgb
Xft.autohint:   False
Xft.hinting:    True
Xft.hintstyle:  auto
Xft.lcdfilter:  lcddefault
!}}}
!{{{ Urxvt
!{{{ Extensions
URxvt.perl-lib:             /usr/lib/urxvt/perl
URxvt.perl-ext-common:      default,matcher,url-select,keyboard-select
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.url-launcher:         /usr/bin/firefox
URxvt.url-select.launcher:  /usr/bin/firefox
!URxvt.url-launcher:         /usr/bin/iron
!URxvt.url-select.launcher:  /usr/bin/iron
URxvt.underlineURLs:        True
URxvt.urlButton:            1
URxvt.colorUL:              #94738c
URxvt.matcher.button:       1
URxvt.matcher.pattern.1:    \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.rend.0:       fg7
!}}}
!{{{ Font
URxvt.font:         -misc-tamsyn-medium-*-*-*-14-*-*-*-*-*-*-*
URxvt.boldFont:     -misc-tamsyn-bold-*-*-*-14-*-*-*-*-*-*-*
!URxvt.boldFont:     -misc-tamsyn-bold-r-normal-*-15-*-*-*-*-*-iso8859-1
!URxvt.font:         -*-clean-medium-r-normal-*-17-*-*-*-*-*-*-*
!}}}
!{{{ Options
URxvt.internalBorder:       0
URxvt.loginShell:           True
URxvt.urgentOnBell:         True
URxvt.colorMode:            on
URxvt.cursorBlink:          True
URxvt.cursorUnderline:      False
URxvt.cursorColor:          #ffaf5f
URxvt.underlineColor:       #5a667f
URxvt.highlightColor:       #5a667f
URxvt.highlightTextColor:   #000000
URxvt.scrollBar:            False
URxvt.borderless:           True
URxvt.scrollstyle:          plain
URxvt.scrollBar_right:      True
URxvt.scrollColor:          #222222
URxvt.geometry:             80x22
!URxvt.geometry:            103x20
URxvt.saveLines:            10000
URxvt.buffered:             True
!}}}
!}}}
